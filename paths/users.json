{
  "get": {
    "description": "Get all users and it should be called by admin",
    "summary": "get /users",
    "tags": [
      "Hub"
    ],
    "operationId": "get /users",
    "security": [
      {
        "ApiKeyAuth": [],
        "bearerAuth": []
      }
    ],
    "parameters": [
      {
        "in": "query",
        "name": "id",
        "description": "id. case sensitive",
        "required": false,
        "schema": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "uuid",
            "example": "b2af7e51-03b1-4986-a0c2-28411a4be8cb"
          }
        }
      },
      {
        "in": "query",
        "name": "account",
        "description": "account. case sensitive",
        "required": false,
        "schema": {
          "type": "string",
          "format": "email",
          "example": "abc@gmail.com"
        }
      },
      {
        "in": "query",
        "name": "is_active",
        "description": "status. case sensitive",
        "required": false,
        "schema": {
          "type": "array",
          "items": {
            "type": "boolean",
            "example": true
          }
        }
      }
    ],
    "deprecated": false,
    "responses": {
      "200": {
        "description": "An array of object containing information of one or more users",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "$ref": "../schemas/users/get/users.json"
                },
                "paging": {
                  "$ref": "../schemas/common/paging.json"
                }
              }
            },
            "examples": {
              "$ref": "../examples/users/get/users.json"
            }
          }
        }
      }
    }
  },
  "post": {
    "description": "post users and it should be called by admin",
    "summary": "post /users",
    "tags": [
      "Hub"
    ],
    "operationId": "post /users",
    "security": [
      {
        "ApiKeyAuth": [],
        "bearerAuth": []
      }
    ],
    "parameters": [
      {
        "in": "query",
        "name": "password_hashed",
        "description": "password hashed or not",
        "required": false,
        "schema": {
          "type": "boolean",
          "example": true
        }
      }
    ],
    "requestBody": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "../schemas/users/post/users.json"
          }
        }
      },
      "required": true
    },
    "deprecated": false,
    "responses": {
      "200": {
        "description": "An array of insert ids",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../schemas/common/ids.json"
            },
            "examples": {
              "$ref": "../examples/common/ids.json"
            }
          }
        }
      }
    }
  }
}